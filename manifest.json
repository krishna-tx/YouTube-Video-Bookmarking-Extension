{
    "manifest_version": 3,
    "name": "YouTube Video Bookmarker",
    "description": "Extension that allows user to add bookmarks to YouTube videos.",
    "version": "1.0",
    "permissions": ["tabs", "scripting"],
    "host_permissions": ["https://www.youtube.com/watch*"],
    "action":{
        "default_popup": "popup.html",
        "default_title": "Youtube Video Bookmarker"
    },
    "background": {
        "service_worker": "background.js"
    }
}